{
    "name": "bengor-file/file-fp",
    "type": "library",
    "description": "BenGorFile library written in functional programming",
    "keywords": [
        "file",
        "component",
        "ddd",
        "fp"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Beñat Espiña Diaz",
            "email": "benatespina@gmail.com",
            "homepage": "https://benatespina.com"
        },
        {
            "name": "Gorka Laucirica Ibarra",
            "email": "gorka.lauzirika@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "BenGorFile\\": "src/BenGorFile/"
        },
        "files": [
            "src/BenGorFile/Application/UploadFile.php",
            "src/BenGorFile/Domain/Model/File/File.php",
            "src/BenGorFile/Domain/Model/File/FileWasUploaded.php",
            "src/BenGorFile/Domain/Model/File/FilePersistence.php",
            "src/BenGorFile/Infrastructure/Domain/Model/File/InMemoryFileFilesystem.php",
            "src/BenGorFile/Infrastructure/Domain/Model/File/InMemoryFileRepository.php"
        ],
        "exclude-from-classmap": [
            "/spec/"
        ]
    }
}
